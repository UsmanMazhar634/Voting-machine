<section class="setter text-center">
<h2>Apply For Candidate Ship</h2>

<%= form_with model: @request, url: candidate_requests_path, local: true do |f| %>

  <div class="field">
    <%= f.label :party %><br />
    <%= f.text_field :party, autofocus: true, autocomplete: "email" %>
  </div>
  </br>

  <div class=“field”>
      <%= f.hidden_field :voter_id, value: current_user.id %>
  </div>
  <%= f.submit %>

<% end %>
</section>
